---
title: Home
blog: project
---

<div class="project__container">
  <% blog.articles.each do |article| %>
    <div class="project">
      <% link_to article do %>
        <img alt="<%= article.data.title %>" class="project__img" src="<%= article.data.img %>">
        <p class="project__title"><%= article.data.title %></p>
      <% end %>
    </div>
  <% end %>
</div>
